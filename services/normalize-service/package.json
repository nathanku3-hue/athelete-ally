{
  "name": "@athlete-ally/normalize-service",
  "version": "1.0.0",
  "description": "Data normalization service for Athlete Ally",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "npx tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest --config jest.config.js --passWithNoTests",
    "lint": "eslint -c ../../eslint.config.mjs src",
    "lint:fix": "eslint -c ../../eslint.config.mjs src --fix",
    "type-check": "tsc --noEmit",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate"
  },
  "dependencies": {
    "@athlete-ally/event-bus": "*",
    "@athlete-ally/shared-types": "*",
    "@prisma/client": "^5.22.0",
    "nats": "^2.19.0",
    "prisma": "^5.22.0",
    "zod": "^3.25.76",
    "@athlete-ally/contracts": "*",
    "@opentelemetry/api": "^1.7.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.0.0",
    "typescript": "^5.9.2"
  },
  "private": true
}
