{
  "name": "planning-engine",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "test": "vitest run",
    "test:unit": "vitest run --testPathPattern=unit",
    "test:integration": "vitest run --testPathPattern=integration",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@athlete-ally/contracts": "^0.1.0",
    "@athlete-ally/event-bus": "^0.1.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.3",
    "@opentelemetry/exporter-jaeger": "^1.18.1",
    "@opentelemetry/exporter-prometheus": "^0.45.1",
    "@opentelemetry/sdk-node": "^0.45.1",
    "@prisma/client": "^5.7.1",
    "async-mutex": "^0.4.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "nats": "^2.19.0",
    "openai": "^4.20.1",
    "pg": "^8.11.5",
    "prom-client": "^15.1.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/supertest": "^6.0.2",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "prisma": "^5.7.1",
    "supertest": "^7.1.3",
    "tslib": "^2.6.2",
    "tsx": "^4.16.2",
    "typescript": "^5.4.5"
  }
}
