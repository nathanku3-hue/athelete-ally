generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model OuraToken {
  userId       String  @id
  accessToken  String
  refreshToken String
  scope        String?
  expiresAt    BigInt?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

