+      exerciseId: 'squat',
+      rpe: 8,
+      completionRate: 100,
+      timestamp: new Date().toISOString(),
+    };
+    const res = await app.inject({ method: 'POST', url: '/v1/plans/feedback/rpe', payload });
+    expect(res.statusCode).toBe(200);
