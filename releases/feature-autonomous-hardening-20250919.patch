+  it('rejects invalid RPE payload with 400', async () => {
+    (proxyRequest as jest.Mock).mockResolvedValue(okResponse({ success: true, data: {} }));
+    const res = await app.inject({ method: 'POST', url: '/v1/plans/feedback/rpe', payload: { rpe: 12 } });
