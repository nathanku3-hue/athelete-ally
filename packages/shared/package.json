{
  "name": "@athlete-ally/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js",
      "default": "./dist/config/index.js"
    },
    "./config/ports": {
      "types": "./dist/config/ports.d.ts",
      "import": "./dist/config/ports.js",
      "default": "./dist/config/ports.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.js",
      "default": "./dist/logger.js"
    },
    "./metrics": {
      "types": "./dist/metrics.d.ts",
      "import": "./dist/metrics.js",
      "default": "./dist/metrics.js"
    },
    "./fastify-augment": {
      "types": "./dist/fastify-augment.d.ts",
      "import": "./dist/fastify-augment.js",
      "default": "./dist/fastify-augment.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "type-check": "tsc --noEmit",
    "lint": "eslint -c ../../eslint.config.unified.mjs src",
    "lint:fix": "eslint -c ../../eslint.config.unified.mjs src --fix"
  },
  "dependencies": {
    "@athlete-ally/logger": "*",
    "jsonwebtoken": "^9.0.2",
    "prom-client": "^15.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "fastify": "^4.28.1",
    "typescript": "^5.9.2"
  },
  "files": [
    "dist"
  ]
}
