{
  "name": "@athlete-ally/shared-types",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "node": "./src/index.ts"
    },
    "./schemas": {
      "types": "./src/schemas.ts",
      "import": "./src/schemas.ts",
      "require": "./src/schemas.ts",
      "node": "./src/schemas.ts"
    },
    "./telemetry": {
      "types": "./src/telemetry.ts",
      "import": "./src/telemetry.ts",
      "require": "./src/telemetry.ts",
      "node": "./src/telemetry.ts"
    },
    "./config": {
      "types": "./src/config.ts",
      "import": "./src/config.ts",
      "require": "./src/config.ts",
      "node": "./src/config.ts"
    },
    "./server-only": {
      "types": "./src/server-only.ts",
      "import": "./src/server-only.ts",
      "require": "./src/server-only.ts",
      "node": "./src/server-only.ts"
    },
    "./metrics-adapter": {
      "types": "./src/metrics-adapter.ts",
      "import": "./src/metrics-adapter.ts",
      "require": "./src/metrics-adapter.ts",
      "node": "./src/metrics-adapter.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "type-check": "tsc --noEmit",
    "lint": "eslint -c ../../eslint.config.mjs src",
    "lint:fix": "eslint -c ../../eslint.config.mjs src --fix"
  },
  "dependencies": {
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  }
}
