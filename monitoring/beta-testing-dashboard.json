{
  "dashboard": {
    "id": "beta-testing-dashboard",
    "title": "Athlete Ally Beta Testing Dashboard",
    "description": "Real-time monitoring dashboard for beta testing phase",
    "tags": ["beta", "testing", "user-behavior"],
    "timezone": "browser",
    "panels": [
      {
        "id": "user-activity-overview",
        "title": "User Activity Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(api_requests_total[5m]))",
            "legendFormat": "Requests/min"
          },
          {
            "expr": "sum(rate(workout_sessions_total[5m]))",
            "legendFormat": "Sessions/min"
          },
          {
            "expr": "sum(rate(fatigue_assessments_total[5m]))",
            "legendFormat": "Fatigue Assessments/min"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
      },
      {
        "id": "onboarding-funnel",
        "title": "Onboarding Funnel",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(onboarding_step_completed_total{step=\"purpose\"}[5m]))",
            "legendFormat": "Purpose Selection"
          },
          {
            "expr": "sum(rate(onboarding_step_completed_total{step=\"proficiency\"}[5m]))",
            "legendFormat": "Proficiency Assessment"
          },
          {
            "expr": "sum(rate(onboarding_step_completed_total{step=\"availability\"}[5m]))",
            "legendFormat": "Availability Setup"
          },
          {
            "expr": "sum(rate(onboarding_step_completed_total{step=\"equipment\"}[5m]))",
            "legendFormat": "Equipment Selection"
          },
          {
            "expr": "sum(rate(onboarding_step_completed_total{step=\"summary\"}[5m]))",
            "legendFormat": "Summary & Submit"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
      },
      {
        "id": "api-performance",
        "title": "API Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(api_response_time_seconds_bucket[5m]))",
            "legendFormat": "P50 Response Time"
          },
          {
            "expr": "histogram_quantile(0.95, rate(api_response_time_seconds_bucket[5m]))",
            "legendFormat": "P95 Response Time"
          },
          {
            "expr": "histogram_quantile(0.99, rate(api_response_time_seconds_bucket[5m]))",
            "legendFormat": "P99 Response Time"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
      },
      {
        "id": "error-rates",
        "title": "Error Rates",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(api_errors_total[5m])) by (service)",
            "legendFormat": "{{service}} Errors/min"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
      },
      {
        "id": "plan-generation-metrics",
        "title": "Plan Generation Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(plan_generation_requests_total[5m]))",
            "legendFormat": "Plan Generation Requests/min"
          },
          {
            "expr": "sum(rate(plan_generation_success_total[5m]))",
            "legendFormat": "Successful Generations/min"
          },
          {
            "expr": "sum(rate(plan_generation_failures_total[5m]))",
            "legendFormat": "Failed Generations/min"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 }
      },
      {
        "id": "workout-tracking-metrics",
        "title": "Workout Tracking Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(workout_sessions_total[5m]))",
            "legendFormat": "Sessions Started/min"
          },
          {
            "expr": "sum(rate(workout_sessions_completed_total[5m]))",
            "legendFormat": "Sessions Completed/min"
          },
          {
            "expr": "sum(rate(workout_records_total[5m]))",
            "legendFormat": "Records Created/min"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 }
      },
      {
        "id": "user-engagement",
        "title": "User Engagement",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(session_duration_seconds_bucket[5m]))",
            "legendFormat": "Median Session Duration"
          },
          {
            "expr": "sum(rate(fatigue_assessments_total[5m]))",
            "legendFormat": "Fatigue Assessments/min"
          },
          {
            "expr": "sum(rate(personal_records_total[5m]))",
            "legendFormat": "Personal Records Set/min"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 }
      },
      {
        "id": "system-health",
        "title": "System Health",
        "type": "graph",
        "targets": [
          {
            "expr": "up{job=~\"gateway-bff|profile-onboarding|planning-engine|exercises|fatigue|workouts\"}",
            "legendFormat": "{{job}} Status"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "annotations": {
      "list": [
        {
          "name": "Beta Testing Events",
          "enable": true,
          "iconColor": "blue",
          "type": "tags",
          "tags": ["beta-test"]
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(up{job=~\"gateway-bff|profile-onboarding|planning-engine|exercises|fatigue|workouts\"}, job)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    }
  }
}

