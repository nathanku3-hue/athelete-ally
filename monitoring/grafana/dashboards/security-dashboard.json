{
  "dashboard": {
    "id": null,
    "title": "Athlete Ally - Security Dashboard",
    "tags": ["athlete-ally", "security", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Authentication Failures",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"planning-engine\",status=\"401\"}[5m])",
            "legendFormat": "401 Unauthorized"
          },
          {
            "expr": "rate(http_requests_total{job=\"planning-engine\",status=\"403\"}[5m])",
            "legendFormat": "403 Forbidden"
          }
        ],
        "yAxes": [
          {
            "label": "Failed Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Rate Limiting",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"planning-engine\",status=\"429\"}[5m])",
            "legendFormat": "Rate Limited"
          }
        ],
        "yAxes": [
          {
            "label": "Rate Limited Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Suspicious Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"planning-engine\",status=~\"4..\"}[5m])",
            "legendFormat": "Client Errors"
          },
          {
            "expr": "rate(http_requests_total{job=\"planning-engine\",status=~\"5..\"}[5m])",
            "legendFormat": "Server Errors"
          }
        ],
        "yAxes": [
          {
            "label": "Error Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Request Sources",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, count by (client_ip) (http_requests_total{job=\"planning-engine\"}))",
            "legendFormat": "Top IPs"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
