{
  "name": "athelete-ally",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.9.3",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "jest --testPathPattern=__tests__/e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "turbo": "turbo",
    "check-ports": "tsx scripts/check-ports.ts",
    "predev": "npx tsx ./scripts/check-ports.ts 3000",
    "dev:check": "npm run check-ports && npm run dev",
    "build:all": "turbo run build",
    "test:all": "turbo run test",
    "type-check:all": "turbo run type-check",
    "lint:all": "turbo run lint",
    "test:e2e": "node tests/e2e/run-e2e-tests.js",
    "test:e2e:watch": "npx jest tests/e2e --config=tests/e2e/jest.config.js --watch",
    "test:container": "docker compose -f preview.compose.yaml run --rm",
    "preview:check": "tsx scripts/check-ports.ts",
    "preview:up": "npm run preview:check && docker compose -f preview.compose.yaml up --build",
    "preview:down": "docker compose -f preview.compose.yaml down -v",
    "config:validate": "node scripts/validate-config.js",
    "config:check": "npm run config:validate",
    "config:test": "node scripts/test-config-integration.js",
    "config:test-integration": "npm run config:test",
    "config:validate-contract": "node scripts/validate-config-contract.js",
    "config:debug": "node scripts/debug-config-integration.js",
    "test:suite:validate": "node scripts/validate-test-suite.js",
    "test:suite:100": "npm run test:suite:validate",
    "db:test": "node scripts/test-db-connection.js",
    "redis:test": "node scripts/test-redis-connection.js",
    "services:health-check": "node scripts/health-check-all.js"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "idb": "^8.0.3",
    "immer": "^10.1.3",
    "jest": "^30.1.3",
    "lucide-react": "^0.542.0",
    "next": "^14",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.22.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.19",
    "eslint": "^8",
    "eslint-config-next": "^14",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.16.2",
    "turbo": "^2.0.3",
    "typescript": "^5"
  },
  "workspaces": [
    "apps/*",
    "services/*",
    "packages/*"
  ]
}
